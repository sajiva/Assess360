.hint-container{
  padding:10px;
}
.hint-input-container{
  margin-top:10px;
}
 .hint-input-label,
 .hints-checkbox-label{
  color:rgb(160,160,160);
  font-family:arabic-indic;
  font-size:18px;
  letter-spacing:1px;

}

.hint-input-label{
  font-size:100px;
}
.hint-container .hints-checkbox-select option{
   padding:10px;
   border-radius:10px;
}
.hint-container .hints-checkbox-select{
  cursor:pointer;
  margin-left: 10px;
  font-size: 0.7em;
  padding:5px 10px;
  color:rgb(160,160,160);
  border-radius:5px;
  font-size:15px;
  box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
  border:none;

}
.hint-container .hints-checkbox-select:active{
  border:none;
}
.hint-container .hint-input{
  margin-left: 10px;
  font-size: 20px;
  border-radius:4px;
  width: 300px;
  display:block;
  font-family:arabic-indic;
  padding:10px;
  box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
  border:none;
  color:rgb(160,160,160);
}
.hint-container input[type='submit']{
  background:white;
  border:none;
  box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
  border-radius:6px;
  padding:5px 10px;
  margin:10px;
  color:rgb(160,160,160);
  font-size:18px;
  cursor:pointer;
  transition:all 500ms;
}
.hint-container input[type='submit']:hover
{
  background:rgb(250,250,250);
  color:rgb(10,10,10);
}


 .parse-button {
    background-color: gray;
    color: white;
    padding: 14px 20px;
    margin: 8px 0;
    border: none;
    cursor: pointer;
    width: 10%;
  box-shadow:0px 1px 1px rgba(0,0,0,.7), inset 0px 1px rgba(255,255,255,.7);
}

.login-button {
  position:relative;
  left:50%;
  transform:translateX(-50%);
  background:white;
  border:none;
  box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
  border-radius:10px;
  padding:5px 10px;
  margin-top:20px;
  color:rgb(88, 88, 88);
  font-size:18px;
  cursor:pointer;
  font-family:"Trajan Pro";
  transition:all 500ms;
}


.login-button:hover .parse-button:hover .submit-emails-button:hover {
  background:rgb(250,250,250);
  color:rgb(10,10,10);
  box-shadow:0px 0px 20px rgba(255,255,255,1),inset 0px 0px 3px rgba(0,0,0,.7);
}

